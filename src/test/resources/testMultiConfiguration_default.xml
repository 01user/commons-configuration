<?xml version="1.0" encoding="ISO-8859-1" ?>
<configuration>
  <colors>
    <background>#40404040</background>
    <text>#000000</text>
    <header>#444444</header>
    <link normal="#040404" visited="#404040"/>
    <default>${colors.default}</default>
  </colors>
  <rowsPerPage>50</rowsPerPage>
  <buttons>
    <name>OK-4,Cancel-4,Help-4</name>
  </buttons>
  <numberFormat pattern="###\,###.##"/>
  <!-- Comma delimited lists -->
  <split>
    <list1>a,b,c</list1>
    <list2>a\,b\,c</list2>
    <list3 values="a,b,c"/>
    <list4 values="a\,b\,c"/>
  </split>
  <Channels>
    <Channel id="1">
      <Name>Channel 1</Name>
      <ChannelData>test 1 data</ChannelData>
    </Channel>
    <Channel id="2">
      <Name>Channel 2</Name>
      <ChannelData>test 2 data</ChannelData>
    </Channel>
  </Channels>
</configuration>